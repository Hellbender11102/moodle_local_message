{{!
    @template local_message/manage

    Template for the contents of the add contact button on the user's profile page.

    Context variables required for this template:
    * none

    Example context (json):
    {
    }
}}
<h1>{{#str}} headline, local_message {{/str}}</h1>
<table>
    <tr>
        <th>{{#str}} tableHeadOne, local_message {{/str}}</th>
        <th>{{#str}} tableHeadTwo, local_message {{/str}}</th>
    </tr>
    {{#messages}}
        <tr>
            <td class="ml-0">{{messagetext}}</td>
            <td class="mr-auto">{{messagetype}}</td>
        </tr>
    {{/messages}}
</table>
<hr>
<input type="button" class="btn btn-primary" onclick="location.href='{{editurl}}'"
       value=" {{#str}} btnEdit, local_message {{/str}}">
